QLineEdit:enabled {
	border: 1px solid #80ADADAD;
	border-radius: 1px;
	padding: 2px;
	/*background: transparent;*/
	selection-background-color: #4d4d4d;
	color: #F0F0F0;
}
QLineEdit:disabled {
	border: 1px solid #5080d4ff;
	border-radius: 1px;
	padding: 2px;
	background: transparent;
	selection-background-color: #4d4d4d;
	color: #4d4d4d;
}
QCheckBox {
	spacing: 2px; 
	color: #D0D0D0;
}

QCheckBox::indicator {
	width: 15px;
	height: 15px;
	color: #D0D0D0;
}

QCheckBox::indicator:unchecked {
	image: url(:/Portable/Resources/unchecked.png);
}

QCheckBox::indicator:checked {
	image: url(:/Portable/Resources/checked.png);
}

QPushButton:enabled{
	border-style: solid;
	border-width: 1px;
	color: #D0D0D0;
	border-color: #80007ACC;
	padding: 5px;	
	min-height: 12px; 
	border-radius:1px;
	background-color:transparent;
}

QPushButton:enabled:hover{ 
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #636363, stop:1 #636363);
}
QPushButton:enabled:checked{ 
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4d4d4d, stop:1 #4d4d4d);
}
QPushButton:enabled:checked:hover{ 
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4d4d4d, stop:1 #4d4d4d);
}
QPushButton:enabled:hover:pressed{ 
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4d4d4d, stop:1 #4d4d4d);
}
QPushButton:disabled{
	border-style: none;
	border-width: 0px;
	color: #4D4D4D;
	padding: 5px;	
	min-height: 12px;
	border-radius:1px;
	background-color:#4d4d4d;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #333333, stop:1 #333333); 
}

QPushButton#btnMenu_Min,QPushButton#btnMenu_Max,QPushButton#btnMenu_Close{
	background-color: transparent;
	border-style:none;
	border-width:0px;
}

QPushButton#btnMenu_Min>CPC_Icon:enabled,QPushButton#btnMenu_Max>CPC_Icon:enabled,QPushButton#btnMenu_Close>CPC_Icon:enabled{
	color:#F0F0F0;
	border-style: none;
	border-width:0px;
}
QPushButton#btnMenu_Min:enabled:hover,QPushButton#btnMenu_Max:enabled:hover{
	background-color:#50ADADAD;
	border-style: none;
	border-width:0px;
}
QPushButton#btnMenu_Close:enabled:hover{
	background-color:#50e60101;
	border-style: none;
	border-width:0px;
}
/* === QProgressBar === */
QProgressBar {
	border-radius: 1px;
	text-align: center;
	border: 1px solid #9080ffff;
	/* background-color:#022523; */
	background-color:transparent;
}

QProgressBar::chunk {
	width: 1px; 
	margin: 0.5px;
	background-color: #80ffff;
}
CPC_Progress {
	border-radius: 1px;
	text-align: center;
	border: 1px solid #9080ffff;
	/* background-color:#022523; */
	background-color:transparent;
}

CPC_Progress::chunk {
	width: 1px; 
	margin: 0.5px;
	background-color: #80ffff;
}
QSpinBox
{
    border: 1px solid #80ADADAD;
	border-radius: 1px;
	padding: 2px;
	background: transparent;
	selection-background-color: #8080ffff;
	color:#F0F0F0;
}
QDoubleSpinBox
{
	color:#F0F0F0;
    border: 1px solid #80ADADAD;
	border-radius: 1px;
	padding: 2px;
	padding-right:20px;
	/*background: transparent;*/
	selection-background-color: #8080ffff;
}
QRadioButton {
	spacing: 4px;
	/*color:#D0D0D0;
	background-color:transparent;*/
	margin:2px;
}
/*CPC_RadioButton {
	spacing: 4px;
	color:#D0D0D0;
	background-color:transparent;
}*/
QRadioButton::indicator:enabled {
	width: 12px; 
	height: 12px; 
	border: 1px solid #84C1FF;
	border-radius:6px;
	background-color:white;
}
QRadioButton::indicator:disabled {
	width: 12px; 
	height: 12px; 
	border: 1px solid #84C1FF;
	border-radius:6px;
	background-color:white;
}
QRadioButton::indicator:checked {
	width: 12px; 
	height: 12px; 
	border: 1px solid #8084C1FF;
	border-radius:6px;
	background-color:#84C1FF;
}
QRadioButton::indicator:unchecked {
	width: 12px; 
	height: 12px; 
	border: 1px solid #8084C1FF;
	border-radius:6px;
	background-color:#white;
}

/* === QScrollBar === */
QScrollBar:vertical {
	width:10px; 
	background-color:rgba(0,0,0,0%); 
	padding-top:10px; 
	padding-bottom:10px;
}

QScrollBar:horizontal {
	height:10px; 
	background-color:rgba(0,0,0,0%); 
	padding-left:10px; padding-right:10px;
}

QScrollBar::handle:vertical {
	width:10px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #636363, stop:1 #575757); 
}

QScrollBar::handle:horizontal {
	height:10px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #636363, stop:1 #575757); 
}

QScrollBar::handle:vertical:hover {
	width:10px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4D4D4D, stop:1 #292929); 
}

QScrollBar::handle:horizontal:hover {
	height:10px;
	background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, stop:0 #4D4D4D, stop:1 #292929); 
}

QScrollBar::add-line:vertical {
	height:10px;
	width:10px;
	subcontrol-position: bottom; 
	subcontrol-origin: margin;
	border-image:url(:/image/add-line_vertical.png);
}

QScrollBar::add-line:horizontal {
	height:10px;
	width:10px;
	subcontrol-position: right;
	subcontrol-origin: margin;
	border-image:url(:/image/add-line_horizontal.png);
}

QScrollBar::sub-line:vertical {
	height:10px;
	width:10px;
	subcontrol-position: top; 
	subcontrol-origin: margin;
	border-image:url(:/image/sub-line_vertical.png);
}

QScrollBar::sub-line:horizontal {
	height:10px;
	width:10px;
	subcontrol-position: left;
	subcontrol-origin: margin;
	border-image:url(:/image/sub-line_horizontal.png);
}

QScrollBar::add-page:vertical,QScrollBar::sub-page:vertical {
	width:10px;
	background: #C0C0C0;
}

QScrollBar::add-page:horizontal,QScrollBar::sub-page:horizontal {
	height:10px;
	background: #C0C0C0;
}

QScrollArea {
	border: 0px ; 
}

