	border-color: #18707070;
	background-color: transparent;
	border-style:solid;
	border-width:1px;
}
QLabel:disabled{
	border-radius:0px;
	color: #4D4D4D;
	border-color: #18707070;
	background-color: transparent;
	border-style:solid;
	border-width:1px;
}
