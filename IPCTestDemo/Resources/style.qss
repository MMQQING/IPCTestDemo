/* === QSlider horizontal=== */
QSlider{
	background-color:transparent;
}
QSlider::groove:horizontal{ 
	border:1px solid #90ADADAD;
	background-color: #022523; 
	height: 14px; 
	border-radius: 0px; 
}

QSlider::sub-page:horizontal {
	border:1px solid #90ADADAD;
	height: 14px; 
	border-radius: 0px;
	background-color: #003434;
}
QSlider::add-page:horizontal {
	border:1px solid #90ADADAD;
	height: 14px; 
	border-radius: 0px;
	background-color: #022523;
}
QSlider::handle:horizontal {
	width: 14px; 
	height: 14px; 
	margin-top: -1px; 
	margin-bottom: -1px; 
	border-width:1px;
	border-style:solid;
	border-color:#88ffff;
	border-radius: 7px;
	background-color: #0F9BA6;
}

CPC_Slider::groove:horizontal{ 
	border:1px solid #90ADADAD;
	background-color: #F0F0F0; 
	height: 2px; 
	border-radius: 0px; 
}

CPC_Slider::sub-page:horizontal {
	border:1px solid #90ADADAD;
	height: 2px; 
	border-radius: 0px;
	background-color: #ADADAD;
}
CPC_Slider::add-page:horizontal {
	border:1px solid #90ADADAD;
	height: 2px; 
	border-radius: 0px;
	background-color: #F0F0F0;
}
CPC_Slider::handle:horizontal {
	width: 5px; 
	height: 5px; 
	margin-top: -6px; 
	margin-bottom: -6px; 
	border-width:1px;
	border-style:solid;
	border-color:#84C1FF;
	border-radius: 3px;
	background-color: #F0F0F0;
}

